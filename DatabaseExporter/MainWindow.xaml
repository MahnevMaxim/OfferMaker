<Window x:Class="DatabaseExporter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DatabaseExporter"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="button" Content="Export users" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.05,0.152" Width="102" Click="button_Click_1"/>
        <Button x:Name="button1" Content="Export nomenclature" HorizontalAlignment="Left" Margin="137,10,0,0" VerticalAlignment="Top" Width="144" Click="button1_Click"/>
        <Button x:Name="button2" Content="Export currency" HorizontalAlignment="Left" Margin="304,10,0,0" VerticalAlignment="Top" Width="196" Click="button2_Click"/>
    </Grid>
</Window>
