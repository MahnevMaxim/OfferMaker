<UserControl x:Class="OfferMaker.Pdf.Views.ShortCalculateOptionEnd"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OfferMaker.Pdf.Views"
             mc:Ignorable="d" 
             xmlns:convert="clr-namespace:OfferMaker.Pdf"
             Background="White">
    <UserControl.Resources>
        <BooleanToVisibilityConverter x:Key="BoolToVisConv"/>
        <convert:NumConvertors x:Key="numConvertors"/>
    </UserControl.Resources>
    <Grid Name="ShortEndGrid" VerticalAlignment="Bottom">
        <Border BorderBrush="Black"
                BorderThickness="0 0 0 1"
                Margin="7,0,0,0"
                Grid.ColumnSpan="5">
        </Border>
    </Grid>
</UserControl>
